{
  "name": "@omnimodel/cli",
  "version": "0.1.0",
  "description": "OmniModel CLI - One tool to query them all",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omnimodel/omniform-cli.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "omniform": "dist/index.js"
  },
  "scripts": {
    "build": "node ../../scripts/build_package.js",
    "start": "node dist/index.js",
    "debug": "node --inspect-brk dist/index.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "config": {
    "sandboxImageUri": "us-docker.pkg.dev/omnimodel-dev/omniform-cli/sandbox:0.1.0"
  },
  "dependencies": {
    "@omnimodel/cli-core": "file:../core",
    "@iarna/toml": "^2.2.5",
    "@types/update-notifier": "^6.0.8",
    "command-exists": "^1.2.9",
    "diff": "^7.0.0",
    "dotenv": "^17.1.0",
    "glob": "^10.4.1",
    "highlight.js": "^11.11.1",
    "ink": "^6.0.1",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-link": "^4.1.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "lowlight": "^3.3.0",
    "mime-types": "^3.0.1",
    "open": "^10.1.2",
    "react": "^19.1.0",
    "read-package-up": "^11.0.0",
    "shell-quote": "^1.8.3",
    "strip-ansi": "^7.1.0",
    "terminal-link": "^3.0.0",
    "update-notifier": "^7.3.1",
    "which": "^5.0.0",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/command-exists": "^1.2.3",
    "@types/diff": "^6.0.0",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.12.5",
    "@types/react": "^19.1.2",
    "@types/shell-quote": "^1.7.5",
    "@types/which": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.21.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
